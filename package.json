{
  "name": "coc-erlang",
  "version": "0.0.1",
  "description": "vim language server extension for coc.nvim",
  "keywords": [
    "coc.nvim",
    "vim",
    "erlang",
    "lsp"
  ],
  "author": "hyhugh <hy1995@gmail.com>",
  "license": "MIT",
  "engines": {
    "coc": "^0.0.62"
  },
  "activationEvents": [
    "onLanguage:vim"
  ],
  "contributes": {
    "configuration": {
      "title": "erlang language server configuration",
      "properties": {
        "erlang.erlang_ls_path": {
          "type": "string",
          "default": "erlang_ls",
          "description": "path of erlang_ls"
        },
        "erlang.trace.server": {
          "type": "string",
          "default": "off",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "description": "Trace level of vim language server"
        }
      }
    }
  },
  "main": "./lib/index.js",
  "scripts": {
    "watch": "tsc -w -p ./",
    "build": "rm -rf ./lib & tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "coc.nvim": "^0.0.72",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "tslib": "^1.9.3",
    "vim-language-server": "^1.2.9"
  }
}
